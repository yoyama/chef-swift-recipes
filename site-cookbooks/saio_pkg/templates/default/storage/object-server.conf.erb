[DEFAULT]
user = <%= @user %>
bind_ip = <%= @storage_local_net_ip %>
bind_port = 6000
workers = 2
log_facility = <%= @log_facility %>
recon_cache_path = /var/cache/swift
eventlet_debug = true

[pipeline:main]
pipeline = recon object-server

[app:object-server]
use = egg:swift#object

[filter:recon]
use = egg:swift#recon

[object-replicator]

[object-updater]

[object-auditor]

